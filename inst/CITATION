url = "https://github.com/thierrygosselin/grur",
doi = "http://dx.doi.org/10.5281/zenodo.154432",
textVersion = paste("Thierry Gosselin (2017). grur: an R package tailored for RADseq data imputation. R package version 0.0.1 https://github.com/thierrygosselin/grur. doi : 10.5281/zenodo.154432")
)
